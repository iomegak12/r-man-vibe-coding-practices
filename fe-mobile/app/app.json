{
  "expo": {
    "name": "TradeEase Mobile",
    "slug": "tradease-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1B4B66"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tradease.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1B4B66"
      },
      "package": "com.tradease.mobile",
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 24,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-dev-client"
    ],
    "scheme": "tradease",
    "extra": {
      "eas": {
        "projectId": "tradease-mobile-2026"
      }
    }
  }
}